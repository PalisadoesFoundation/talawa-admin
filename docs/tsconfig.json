{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": [
      "@docusaurus/module-type-aliases",
      "@docusaurus/types",
      "react",
      "react-dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@theme/*": ["./src/theme/*"],
      "@site/*": ["./*"]
    }
  },
  "include": ["src/**/*", "docusaurus.config.ts", "sidebars.ts"],
  "exclude": ["node_modules", "build", ".docusaurus"]
}
