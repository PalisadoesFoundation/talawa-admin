  },
  "scripts": {
    "serve": "cross-env ESLINT_NO_DEV_ERRORS=true vite --config config/vite.config.ts",
    "build": "tsc && vite build --config config/vite.config.ts",
    "preview": "vite preview --config config/vite.config.ts",
    "test": "vitest run",
    "test:watch": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:shard": "node scripts/run-shard.js",
    "test:shard:coverage": "node scripts/run-shard.js --coverage",
    "eject": "react-scripts eject",
    "lint:check": "eslint \"**/*.{ts,tsx,graphql}\" --max-warnings=0 && python .github/workflows/scripts/eslint_disable_check.py",
    "lint:fix": "eslint --fix \"**/*.{ts,tsx,graphql}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,json,json5,scss,css,graphql,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,json5,scss,css,graphql,yaml,yml}\"",
    "check-tsdoc": "node .github/workflows/check-tsdoc.js",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prepare": "husky",
    "jest-preview": "jest-preview",
    "update:toc": "node scripts/githooks/update-toc.js",
    "lint-staged": "lint-staged --concurrent false",
    "setup": "tsx src/setup/setup.ts",
    "check-localstorage": "tsx scripts/githooks/check-localstorage-usage.ts",
    "postgenerate-docs": "find docs/docs/auto-docs -name 'README.md' -delete && node scripts/docs/fix-repo-url.js",
    "generate-docs": "typedoc && npm run postgenerate-docs && node scripts/docs/fix-readme-links.js",
    "cypress:run": "cypress run && nyc report --reporter=text-summary",
    "cypress:open": "cypress open",
    "start-server": "start-server-and-test serve http://localhost:4321",
    "cy:open": "npm run start-server cypress:open",
    "cy:run": "cross-env CYPRESS_COVERAGE=true npm run start-server cypress:run",
    "check:pom": "node scripts/githooks/check_pom.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cypress/code-coverage": "^3.14.5",
    "@eslint/js": "^9.22.0",
    "@graphql-eslint/eslint-plugin": "^4.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/apollo-upload-client": "^18.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/eslint-plugin": "^1.4.2",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^7.0.3",
    "cypress": "^14.4.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-tsdoc": "^0.4.0",
    "graphql": "^16.10.0",
    "husky": "^9.1.6",
    "jest-environment-jsdom": "^30.2.0",
    "knip": "^5.66.0",
    "lint-staged": "^16.2.3",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "sass": "^1.93.3",
    "start-server-and-test": "^2.0.12",
    "tsx": "^4.19.1",
    "typedoc": "^0.28.1",
    "typedoc-plugin-markdown": "^4.5.0",
    "typescript": "^5.8.3",
    "vite": "^7.1.12",
    "vite-plugin-istanbul": "^7.1.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.2.4"
  },
  "nyc": {
    "reporter": [
      "html",
      "lcov"
    ],
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "all": true,
    "include": [
      "src/screens/**/*.{js,jsx,ts,tsx}",
      "src/components/**/*.{js,jsx,ts,tsx}",
      "src/subComponents/**/*.{js,jsx,ts,tsx}"
    ],
    "exclude": [
      "cypress/**",
      "**/*.cy.*",
      "src/**/*.spec.*",
      "coverage/**"
    ],
    "temp-directory": ".nyc_output"
  },
  "resolutions": {
    "@apollo/client": "^3.4.0-beta.19"
  },
  "engines": {
    "node": ">=24.x"
  },
  "overrides": {
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "whatwg-url": "^14.0.0",
    "core-js": "^3.40.0",
    "sync-fetch": "npm:@ardatan/sync-fetch@0.0.1",
    "rc-color-picker": {
      "react": "^19.1.0",
      "react-dom": "^19.1.0"
    },
    "virtualizedtableforantd4": {
      "react": "^19.1.0",
      "react-dom": "^19.1.0"
    },
    "gulp-header": "^2.0.9"
  },
  "lint-staged": {
    "**/*.{ts, tsx, json, scss, css}": [
      "prettier --write"
    ],
    "**/*.{ts, tsx, json}": "eslint --fix"
  },
  "packageManager": "npm@11.1.0"
}
